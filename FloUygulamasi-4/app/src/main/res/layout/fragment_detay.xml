<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragment.DetayFragment">

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="66dp"
        android:layout_height="62dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="300dp"
        android:src="@drawable/resim_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.069"
        app:layout_constraintStart_toEndOf="@+id/imageView13"
        app:layout_constraintTop_toBottomOf="@+id/textViewDegerY" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarDetay"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/ivAyakkabiResim"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:src="@drawable/resim_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarDetay" />

    <ImageView
        android:id="@+id/imageViewLike"
        android:layout_width="29dp"
        android:layout_height="33dp"
        android:layout_marginStart="350dp"
        android:layout_marginBottom="130dp"
        android:src="@drawable/favorite"
        app:layout_constraintBottom_toBottomOf="@+id/ivAyakkabiResim"
        app:layout_constraintEnd_toEndOf="@+id/ivAyakkabiResim"
        app:layout_constraintStart_toStartOf="@+id/ivAyakkabiResim"
        app:layout_constraintTop_toBottomOf="@+id/toolbarDetay" />

    <ImageView
        android:id="@+id/imageViewShops"
        android:layout_width="26dp"
        android:layout_height="37dp"
        android:layout_marginStart="350dp"
        android:layout_marginBottom="220dp"
        android:src="@drawable/shoptool"
        app:layout_constraintBottom_toBottomOf="@+id/ivAyakkabiResim"
        app:layout_constraintEnd_toEndOf="@+id/ivAyakkabiResim"
        app:layout_constraintStart_toStartOf="@+id/ivAyakkabiResim"
        app:layout_constraintTop_toBottomOf="@+id/toolbarDetay" />

    <ImageView
        android:id="@+id/imageViewShare"
        android:layout_width="24dp"
        android:layout_height="35dp"
        android:layout_marginStart="350dp"
        android:layout_marginBottom="45dp"
        android:src="@drawable/share"
        app:layout_constraintBottom_toBottomOf="@+id/ivAyakkabiResim"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ivAyakkabiResim"
        app:layout_constraintTop_toBottomOf="@+id/toolbarDetay" />

    <TextView
        android:id="@+id/textViewKinetix"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="326dp"
        android:layout_marginBottom="281dp"
        android:fontFamily="sans-serif-black"
        android:text="Kinetix"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivAyakkabiResim" />

    <TextView
        android:id="@+id/textViewTanim"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="249dp"
        android:text="HAWKİNS 3FX Siyah Erkek Günlük Ayakkabı"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewKinetix" />

    <TextView
        android:id="@+id/textViewYeniSezon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="320dp"
        android:layout_marginBottom="190dp"
        android:text="Yenis Sezon"
        app:layout_constraintBottom_toBottomOf="@+id/ivAyakkabiResim"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarDetay" />

    <ImageView
        android:id="@+id/imageViewBack"
        android:layout_width="38dp"
        android:layout_height="32dp"
        android:layout_marginEnd="360dp"
        android:layout_marginBottom="240dp"
        android:src="@drawable/baseline_arrow_back_ios_24"
        app:layout_constraintBottom_toBottomOf="@+id/ivAyakkabiResim"
        app:layout_constraintEnd_toEndOf="@+id/ivAyakkabiResim"
        app:layout_constraintStart_toStartOf="@+id/ivAyakkabiResim"
        app:layout_constraintTop_toBottomOf="@+id/toolbarDetay" />

    <ImageView
        android:id="@+id/imageViewDegerS"
        android:layout_width="20dp"
        android:layout_height="22dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="345dp"
        android:layout_marginBottom="212dp"
        android:src="@drawable/star"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewTanim" />

    <TextView
        android:id="@+id/textViewDegerR"
        android:layout_width="0dp"
        android:layout_height="24dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="15dp"
        android:text="5.0"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/imageViewDegerS"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewTanim" />

    <TextView
        android:id="@+id/textViewDegerY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="254dp"
        android:layout_marginBottom="215dp"
        android:text="3 değerlendirme"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewDegerS"
        app:layout_constraintTop_toBottomOf="@+id/textViewTanim" />

    <ImageView
        android:id="@+id/imageViewBegeni"
        android:layout_width="23dp"
        android:layout_height="19dp"
        android:layout_marginStart="67dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/favoritenew"
        app:layout_constraintStart_toEndOf="@+id/textViewDegerY"
        app:layout_constraintTop_toBottomOf="@+id/textViewTanim" />

    <TextView
        android:id="@+id/textViewBegenY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="214dp"
        android:text="640 kişinin favorisi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewBegeni"
        app:layout_constraintTop_toBottomOf="@+id/textViewTanim"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="337dp"
        android:layout_marginBottom="176dp"
        android:text="Renk: Siyah"
        android:textColor="@color/material_dynamic_neutral10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewDegerS" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="58dp"
        android:layout_height="66dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/resim_3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/tvFiyat"
        android:layout_width="0dp"
        android:layout_height="28dp"
        android:layout_marginStart="22dp"
        android:layout_marginBottom="27dp"
        android:text="549.99"
        android:textColor="?android:attr/colorMultiSelectHighlight"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttonSepeteEkle"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="13dp"
        android:text="Sepete Ekle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>